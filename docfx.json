{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "E:/UnityProjects/Glitch9/AI Development Kit Clean/AI Development Kit Clean/Assets",
          "files": [
            "Glitch9/AIDevKit/Runtime/Scripts/**/*.cs",
            "Glitch9/CoreLib/Runtime/**/*.cs"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "allowCompilationErrors": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "templates/default"
    ],
    "globalMetadata": {
      "_appName": "AI DevKit",
      "_appTitle": "AI DevKit",
      "_enableSearch": true,
      "_appLogoPath": "images/logo.png",
      "pdf": true
    }
  }
}